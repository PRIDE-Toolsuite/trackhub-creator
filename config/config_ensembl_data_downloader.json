{
  "data_downloader": {
    "ensembl_ftp": {
      "base_url": "ftp://ftp.ensembl.org/pub",
      "folder_prefix_release": "release-",
      "folder_name_fasta": "fasta",
      "folder_name_protein_sequences": "pep",
      "folder_name_gtf": "gtf"
    },
    "rewrite_local_path_ensembl_repo": "False",
    "ensembl_file_names": {
      "protein_sequence_file": {
        "file_type": "pep",
        "file_suffixes": ["all", "abinitio"],
        "file_extension": "fa"
      },
      "gtf_file": {
        "file_suffixes": ["", ".abinitio", ".chr", ".chr_patch_hapl_scaff"],
        "file_extension": "gtf"
      }
    }
  }
}